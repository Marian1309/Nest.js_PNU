  "scripts": {
    "prepare": "husky",
    "clean": "rimraf bun.lockb && rimraf dist && rm -rf node_modules",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "nest build",
    "start": "nest start",
